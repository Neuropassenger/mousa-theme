{
	"version": 3,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"layout": {
			"contentSize": "1140px",
			"wideSize": "2200px"
		},
		"color": {
			"custom": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#eeede0",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#c9a050",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#1e1d18",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#f7f6ec",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#6d6d65",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#92938f",
					"name": "Quaternary",
					"slug": "quaternary"
				},
				{
					"color": "#92938f",
					"name": "Quinary",
					"slug": "quinary"
				},
				{
					"color": "#ebe9cf",
					"name": "Senary",
					"slug": "senary"
				}
			]
		},
		"typography": {
			"fluid": false,
			"letterSpacing": true,
			"lineHeight": true,
			"textColumns": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Cormorant Garamond",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": "file:./assets/fonts/cormorant-garamond/700.woff2"
						},
						{
							"fontFamily": "Cormorant Garamond",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": "file:./assets/fonts/cormorant-garamond/400.woff2"
						}
					],
					"fontFamily": "Cormorant Garamond, serif",
					"name": "Cormorant Garamond",
					"slug": "cormorant-garamond"
				}
			],
			"defaultFontSizes": false,
			"fontSizes": [
				{
					"name": "Extra Tiny",
					"slug": "xx-small",
					"size": "14px"
				},
				{
					"name": "Tiny",
					"slug": "x-small",
					"size": "16px"
				},
				{
					"name": "Extra Small",
					"slug": "small",
					"size": "20px"
				},
				{
					"name": "Small",
					"slug": "small-medium",
					"size": "24px"
				},
				{
					"name": "Medium Small",
					"slug": "medium-small",
					"size": "30px"
				},
				{
					"name": "Medium",
					"slug": "medium",
					"size": "32px"
				},
				{
					"name": "Large Medium",
					"slug": "large-medium",
					"size": "36px"
				},
				{
					"name": "Large",
					"slug": "large",
					"size": "40px"
				},
				{
					"name": "Extra Large",
					"slug": "x-large",
					"size": "48px"
				},
				{
					"name": "Huge",
					"slug": "xx-large",
					"size": "64px"
				},
				{
					"name": "Gigantic",
					"slug": "xxx-large",
					"size": "70px"
				},
				{
					"name": "Massive",
					"slug": "xxxx-large",
					"size": "80px"
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--primary)"
		},
		"blocks": {
			"core/image": {
				"elements": {
					"caption": {
						"color": {
							"text": "var(--wp--preset--color--tertiary)"
						},
						"typography": {
							"fontSize": "18px"
						}
					}
				}
			},
			"core/gallery": {
				"elements": {
					"caption": {
						"color": {
							"text": "var(--wp--preset--color--tertiary)"
						},
						"typography": {
							"fontSize": "18px"
						}
					}
				}
			},
			"core/quote": {
				"border": {
					"left": {
						"color": "var(--wp--preset--color--contrast)",
						"style": "solid",
						"width": "2px"
					}
				},
				"spacing": {
					"margin": {
						"left": "0"
					},
					"padding": {
						"left": "40px"
					}
				},
				"elements": {
					"cite": {
						"typography": {
							"fontWeight": "700",
							"fontStyle": "normal"
						}
					}
				}
			},
			"core/separator": {
				"color": {
					"background": "var(--wp--preset--color--senary)"
				}
			},
			"core/button": {
				"color": {
					"background": "var(--wp--preset--color--primary)"
				}
			}
		},
		"elements": {
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"textDecoration": "none"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxx-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small-medium)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			}
		},
		"spacing": {
			"padding": {
				"bottom": "0px",
				"left": "0px",
				"right": "0px",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--cormorant-garamond)",
			"fontSize": "var(--wp--preset--font-size--small-medium)"
		}
	}
}